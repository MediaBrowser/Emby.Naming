<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyVersion>1.0.8.0</AssemblyVersion>
    <FileVersion>1.0.8.0</FileVersion>
    <Version>1.0.8-beta</Version>
    <PackageId>MediaBrowser.Naming</PackageId>
    <Description>A file naming engine used by Emby Server.</Description>
    <PackageIconUrl>http://www.mb3admin.com/images/mb3icons1-1.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/MediaBrowser/Emby.Naming</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MediaBrowser/Emby.Naming</RepositoryUrl>
    <Company>Emby</Company>
    <Authors>Emby</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MediaBrowser.Common" Version="3.3.4-beta" />
  </ItemGroup>

</Project>
